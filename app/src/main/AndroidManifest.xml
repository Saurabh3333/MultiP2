<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fridaygmail.saurabh.multip">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login"
            android:screenOrientation="sensorPortrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QuizMainPage"
            android:screenOrientation="sensorPortrait"/>
        <activity android:name=".QuestionFeedMainPage"
            android:screenOrientation="sensorPortrait"/>
        <activity android:name=".Questions"
            android:screenOrientation="sensorPortrait"/>
        <activity android:name=".ScoreCard"
            android:screenOrientation="sensorPortrait"/>
        <activity android:name=".MainActivity"
            android:screenOrientation="sensorPortrait"/>
        <activity android:name=".Signup"
            android:screenOrientation="sensorPortrait"/>
        <activity android:name=".FirstPageAfterLogin"
            android:screenOrientation="sensorPortrait"/>
        <activity android:name=".WebPage"
            android:screenOrientation="sensorPortrait"/>
        <activity android:name=".Camera"
            android:screenOrientation="sensorPortrait"/>
        <activity android:name=".Calculator"
            android:screenOrientation="sensorPortrait"/>
        <activity android:name=".MagicalMusicPlayer"
            android:screenOrientation="sensorPortrait"/>
        <activity android:name=".MediaPlayerPage"
            android:screenOrientation="sensorPortrait"></activity>
    </application>

</manifest>